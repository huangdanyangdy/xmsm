
page {
  font-family:PingFang SC;
}
.comp-tabbar{
  position:fixed;
	bottom:0;
  display:flex;
  align-items:center;
  width:100%;
  height:100rpx;
  text-align:center;
  font-size:24rpx;
  box-sizing:border-box;
  box-shadow:0px 5px 21px 0px rgba(6,0,1,0.14);
  z-index: 999;
}
.comp-tabbar .tabbarItem{
  flex:1;
  height:inherit;
  display:flex;
  flex-direction:column;
  align-items:center;
  justify-content:center;
}
.comp-tabbar .tabbarItem .icon{
margin-top: 0.13rpx;
  width:45rpx;
  height:45rpx;
}
.icon {
  margin-bottom: 5rpx;
}